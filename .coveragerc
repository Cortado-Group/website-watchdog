# Coverage configuration

[run]
source = .
omit =
    test_*.py
    setup.py
    */__pycache__/*
    */venv/*
    */env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
skip_empty = True

[html]
directory = coverage_html
